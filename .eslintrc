{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        }
    },
    "plugins": [
        "import", "react"
    ],
    "settings": {
        "import/ignore": [
            "node_modules",
            "\\.(json|css|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$",
        ],
        "import/extensions": [".js"],
        "import/resolver": {
            node: {
                extensions: [".js", ".json"]
            }
        }
    },
    "rules": {
        "no-console": 0,
        "no-underscore-dangle": 1,
        "quotes": [2, "single"]
    }
}